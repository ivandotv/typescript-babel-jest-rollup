{
  "name": "untitled",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/unpkg/mylib.js",
  "jsdelivr": "dist/unpkg/mylib.js",
  "sideEffects": true,
  "types": "dist/types/",
  "files": [
    "dist",
    "src"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "changelog": "auto-changelog --template changelog-template.hbs -p -u --commit-limit false",
    "changelog-debug": "auto-changelog --template changelog-template.hbs -p --template json --output changelog-data.json",
    "nodemon": "./node_modules/.bin/nodemon",
    "fix:src": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "fix:tests": "eslint --fix \"./tests/**/*.{js,jsx,ts,tsx}\"",
    "fix": "npm-run-all -p fix:*",
    "docs": "del docs/api && typedoc --theme markdown --readme none --excludePrivate --excludeNotExported --out docs/api ./src",
    "size": "npm run size-limit",
    "size-why": "npm run size-limit --why",
    "test": "cross-env BABEL_ENV=test jest --verbose --coverage --no-cache",
    "test:watch": "cross-env BABEL_ENV=test jest --watch",
    "ci:test": "jest --runInBand --ci --reporters=default --reporters=jest-junit --coverage --coverageDirectory=coverage",
    "type-check": "tsc -p tsconfig.check-types.json",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "del ./dist/types && tsc -p tsconfig.build-types.json --outDir ./dist/types",
    "build:unpkg": "del ./dist/unpkg && rollup -c",
    "build:cjs": "del ./dist/cjs && cross-env BABEL_ENV=cjs babel src --out-dir dist/cjs --extensions \".ts,.tsx,js,jsx\" --source-maps",
    "build:esm": "del ./dist/esm && cross-env BABEL_ENV=esm babel src --out-dir dist/esm --extensions \".ts,.tsx,js,jsx\" --source-maps",
    "build:watch": "del ./dist/csj && cross-env BABEL_ENV=test babel src --out-dir dist/cjs --extensions \".ts,.tsx,js,jsx\"",
    "build": "npm run build:types && npm run build:cjs && npm run build:esm && npm run build:unpkg"
  },
  "size-limit": [
    {
      "path": "dist/csj/index.js"
    }
  ],
  "jest-junit": {
    "outputDirectory": "./reports/junit",
    "outputName": "test-results.xml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint --fix"
    ],
    "tests/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivandotv/node-module-ts.git"
  },
  "bugs": {
    "url": "https://github.com/ivandotv/node-module-ts/issues"
  },
  "homepage": "https://github.com/ivandotv/node-module-ts#readme",
  "keywords": [],
  "author": "Ivan Vlatkovic",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/types": "^7.8.6",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@size-limit/preset-small-lib": "^4.0.2",
    "@types/jest": "^25.1.3",
    "@types/lodash.filter": "^4.6.6",
    "@types/lodash.orderby": "^4.6.6",
    "@types/lodash.shuffle": "^4.2.6",
    "@types/node": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-preset-minify": "^0.5.1",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.0",
    "del-cli": "^3.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prettier-standard": "^16.1.0",
    "rollup": "^1.32.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-terser": "^5.2.0",
    "size-limit": "^4.0.2",
    "standardx": "^5.0.0",
    "typedoc": "^0.16.11",
    "typedoc-plugin-markdown": "^2.2.17",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
