{
  "extends": ["config:base"],
  "rangeStrategy": "replace",
  "semanticCommits": true,
  "labels": ["dependencies"],
  "packageRules": [
    {
      "updateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "packagePatterns": ["^eslint"],
      "groupName": "eslint packages",
      "addLabels": ["eslint"]
    }
  ]
}
